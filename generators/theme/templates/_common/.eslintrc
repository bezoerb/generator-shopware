{
  "extends": "xo",
  "env": {
    "node": false,
    "es6": false,
    "browser": true,
    "commonjs": true
  },
  "rules": {
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-invalid-this": 0,
    "no-else-return": ["error", { "allowElseIf": true }],
    "comma-dangle": ["error", "always-multiline"]
  },
  "globals": {
    "jQuery": false,
    "$": false
  }
}
